<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shijan.musicmatcher">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dd.sample.SignInActivity"
            android:configChanges="orientation|screenSize|smallestScreenSize"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dd.sample.MessageActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.dd.sample.UploadActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.dd.sample.StateSampleActivity"
            android:screenOrientation="portrait" />


        <activity android:name=".FetchSong" />
        <activity android:name=".ActivityFirst" />
        <activity android:name=".MainActivity" />

    </application>

</manifest>